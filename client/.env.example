# App Configuration
VITE_ENVIRONMENT=dev                    # "dev", "development", "prod", "production"
VITE_PORT=5173                         # Optional: Development server port
VITE_URI=https://www.zap-quiz.com/     # Optional: Production URI (ignored in dev)
VITE_BASE_URL=/                        # Optional: Base URL for the app
VITE_ROUTER_BASE_NAME=                 # Optional: Router base name
VITE_LOAD_PATH_i18=/locales/{{lng}}/{{ns}}.json  # Optional: i18n load path

# Kinde Authentication
VITE_KINDE_CLIENT_ID=                  # Your Kinde client ID
VITE_KINDE_DOMAIN=                     # Your Kinde domain
VITE_KINDE_REDIRECT_URI=               # Optional: Override redirect URI
VITE_KINDE_LOGOUT_URI=                 # Optional: Override logout URI

# Firebase Configuration
VITE_FIREBASE_APIKEY=                  # Your Firebase API key
VITE_FIREBASE_AUTHDOMAIN=              # Your Firebase auth domain
VITE_FIREBASE_PROJECTID=               # Your Firebase project ID
VITE_FIREBASE_STORAGEBUCKET=           # Your Firebase storage bucket
VITE_FIREBASE_MESSAGINGSENDERID=       # Your Firebase messaging sender ID
VITE_FIREBASE_APPID=                   # Your Firebase app ID
VITE_FIREBASE_DATABASEURL=             # Your Firebase database URL
VITE_FIREBASE_MEASUREMENTID=           # Your Firebase measurement ID 